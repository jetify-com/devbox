{
  "install_stage": {
    "command": "poetry add pex -n --no-ansi \u0026\u0026 poetry install --no-dev -n --no-ansi",
    "input_files": [
      "."
    ]
  },
  "build_stage": {
    "command": "poetry run pex . -o app.pex -m poetry_with_main --validate-entry-point"
  },
  "start_stage": {
    "command": "python ./app.pex",
    "input_files": [
      "."
    ]
  },
  "dev_packages": [
    "python310",
    "poetry"
  ],
  "runtime_packages": [
    "python310"
  ]
}