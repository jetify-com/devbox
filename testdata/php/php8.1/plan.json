{
  "dev_packages": [
    "php81",
    "php81Packages.composer"
  ],
  "runtime_packages": [
    "php81",
    "php81Packages.composer"
  ],
  "install_stage": {
    "command": "composer install --no-dev --no-ansi",
    "input_files": [
      "."
    ]
  },
  "build_stage": {
    "command": ""
  },
  "start_stage": {
    "command": "php -S 0.0.0.0:8080 -t public",
    "input_files": [
      "."
    ]
  },
  "package_extensions": [
    "php81 = pkgs.php81.withExtensions ({ enabled, all }: enabled ++ (with all; [ imagick mbstring ]));"
  ]
}
